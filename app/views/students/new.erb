<h1>New Student</h1>
<h1>Student Form</h1>
<%= form_for @student do |f| %>
  <%= f.label 'First Name' %>
  <%= f.text_field :first_name, placeholder: 'First Name' %><br>
  <%= f.label 'Last Name' %>
  <%= f.text_field :last_name, placeholder: 'Last Name' %><br>
  <br>
  <%= f.submit "Create Student" %>
<% end %>
