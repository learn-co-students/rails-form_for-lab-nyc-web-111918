<h1>Edit School Class</h1>

<%= form_for (@school_class) do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :room_number %>
  <%= f.number_field :room_number %>
  <%= f.submit %>
<% end %>

<%= form_for @school_class, method: :delete do |f| %>
  <%= f.submit :value => "Delete School class" %>
<% end %>
